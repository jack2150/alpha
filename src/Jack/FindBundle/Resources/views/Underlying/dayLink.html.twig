<table>
    <tr>
        <td>
            <b style="float: left; width: 100px; height:40px;">More Days:</b>
            {% for searchLink in searchLinks %}
                {% if searchLink.url == '#' %}
                    <div style="float: left; width: 40px;">
                        [{{ searchLink.day }}]
                    </div>
                {% else %}
                    <a style="float: left;" href="{{ path('jack_find_underlying_result_findbyday',
                    {'action': searchLink.url, 'symbol': symbol, 'day': searchLink.day }) }}">
                        <div style="width: 40px;">
                            {{ searchLink.day }}
                        </div>
                    </a>
                {% endif %}
            {% endfor %}
        </td>
    </tr>
</table>
<br/>



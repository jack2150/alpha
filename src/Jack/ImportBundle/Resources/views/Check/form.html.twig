{% extends "AcmeDemoBundle::layout.html.twig" %}

{% block head %}
    <link rel="icon" sizes="16x16" href="{{ asset('favicon.ico') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/acmedemo/css/demo.css') }}"/>
{% endblock %}

{% block title %}Import Bundle: Check Import Data{% endblock %}

{% block content %}
    <h2 style="background-color: darkcyan">Check Imported</h2>

    {{ form_start(form) }}
    {{ form_errors(form) }}

    <div>
        {{ form_label(form.name, 'Symbol Name:') }}
        {{ form_errors(form.name) }}
        {{ form_widget(form.name) }}
        {{ form_widget(form.check) }}
    </div>

    <span style="font-size: 0.8em; color: darkorange">
        ** Latest import into database.
    </span>

    {{ form_end(form) }}

{% endblock %}
